def sum_dig_pow(a, b): # range(a, b + 1) will be studied by the function
    ans=[]
    for i in range(a,b+1):
        newst=str(i)
        t=0
        pow=1
        for j in range(0,len(newst)):
            t=t+int(newst[j])**pow
            pow=pow+1
        if(t==i):
            ans.append(t)
    return ans        
        
